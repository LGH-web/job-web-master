urn {\n                article: {},\n                channel: {}\n            }\n        },\n        mounted() {\n            this.getArticle(this.$route.params.id);\n        },\n        methods: {\n            getArticle(id) {\n                article_detail(id).then(res => {\n                    this.article = res.data;\n                    channel_detail(this.article.channelId).then(res => {\n                        this.channel = res.data;\n                    })\n                })\n            }\n        }\n    }\n</script>\n\n<style scoped>\n\n</style>"]}]}